<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.vishv.a2101015270">

    <application
        android:allowBackup="true"
        android:icon="@drawable/hospital_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/hospital_logo"
        android:supportsRtl="true"
        android:theme="@android:style/Theme.Material"
        android:fullBackupContent="@xml/backup_descriptor"
        tools:ignore="GoogleAppIndexingWarning">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".DashboardActivity" />
        <activity android:name=".AddPatientActivity" />
        <activity android:name=".ViewPatientActivity" />
        <activity android:name=".AddTestActivity" />
        <activity android:name=".ViewTestActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>



</manifest>